{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "laravel/framework": "4.1.*",
        "twitter/bootstrap": "*",//php artisan asset:publish --path="vendor/twitter/bootstrap/dist/" bootstrap
        "davejamesmiller/laravel-breadcrumbs": "~2.2.0",
        "cviebrock/eloquent-sluggable": "1.0.*",
        "hybridauth/hybridauth": "dev-master",
        "codesleeve/stapler": "dev-master",
        "rtconner/laravel-tagging": "dev-master"//php artisan migrate --package=rtconner/laravel-tagging
    },
    "require-dev": {
        "way/generators": "2.*"
    },
    "autoload": {
        "classmap": [
            "app/commands",
            "app/controllers",
            "app/models",
            "app/database/migrations",
            "app/database/seeds",
            "app/tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-install-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize"
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ]
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "stable"
}


